            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>-->
        <!--<table tableName="ps_admin_party" domainObjectName="ps.PsAdminParty"
               mapperName="ps.PsAdminPartyMapper">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>-->
        <!--<table tableName="ps_member" domainObjectName="ps.PsMember"
               mapperName="ps.PsMemberMapper">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>-->
        <!--<table tableName="ps_party" domainObjectName="ps.PsParty"
               mapperName="ps.PsPartyMapper">
