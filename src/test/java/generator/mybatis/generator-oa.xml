<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <!-- 引入配置文件 -->
    <properties resource="spring.properties"/>
    <context id="context1" targetRuntime="MyBatis3">
        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin"></plugin>
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>
        <plugin type="org.mybatis.generator.plugins.UnmergeableXmlMappersPlugin"></plugin>
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!-- jdbc连接 -->
        <jdbcConnection driverClass="${jdbc_driverClassName}"
                        connectionURL="${jdbc_url}" userId="${jdbc_user}"
                        password="${jdbc_password}" />
        <javaModelGenerator targetPackage="domain" targetProject="src/main/java">
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="persistence" targetProject="src/main/java">
        </sqlMapGenerator>
        <javaClientGenerator targetPackage="persistence" targetProject="src/main/java" type="XMLMAPPER">
        </javaClientGenerator>

        <!--协同办公-->
        <!--<table tableName="oa_task" domainObjectName="oa.OaTask" mapperName="oa.OaTaskMapper">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
            <columnOverride column="content" jdbcType="VARCHAR"></columnOverride>
        </table>
        <table tableName="oa_task_view" domainObjectName="oa.OaTaskView" mapperName="oa.OaTaskViewMapper">
            <columnOverride column="user_id" jdbcType="INTEGER" javaType="Integer"></columnOverride>
            <columnOverride column="content" jdbcType="VARCHAR"></columnOverride>
            <columnOverride column="is_publish" jdbcType="BOOLEAN" javaType="Boolean"></columnOverride>
            <columnOverride column="is_delete" jdbcType="BOOLEAN" javaType="Boolean"></columnOverride>
            <columnOverride column="status" jdbcType="TINYINT" javaType="Byte"></columnOverride>
        </table>-->
        <!--<table tableName="oa_task_admin" domainObjectName="oa.OaTaskAdmin" mapperName="oa.OaTaskAdminMapper">
            <columnOverride column="types" jdbcType="VARCHAR"></columnOverride>
        </table>-->

       <!-- <table tableName="oa_task_file" domainObjectName="oa.OaTaskFile" mapperName="oa.OaTaskFileMapper">
        </table>
        <table tableName="oa_task_msg" domainObjectName="oa.OaTaskMsg" mapperName="oa.OaTaskMsgMapper">
            <columnOverride column="content" jdbcType="VARCHAR"></columnOverride>
        </table>-->
        <!--<table tableName="oa_task_user" domainObjectName="oa.OaTaskUser" mapperName="oa.OaTaskUserMapper">
            <columnOverride column="content" jdbcType="VARCHAR"></columnOverride>
        </table>-->
         <table tableName="oa_task_user_view" domainObjectName="oa.OaTaskUserView" mapperName="oa.OaTaskUserViewMapper">
            <columnOverride column="task_user_ids" jdbcType="VARCHAR"></columnOverride>
             <columnOverride column="content" jdbcType="VARCHAR"></columnOverride>
             <columnOverride column="task_content" jdbcType="VARCHAR"></columnOverride>
            <columnOverride column="has_report" jdbcType="BOOLEAN" javaType="Boolean"></columnOverride>
            <columnOverride column="is_back" jdbcType="BOOLEAN" javaType="Boolean"></columnOverride>
            <columnOverride column="status" jdbcType="TINYINT" javaType="Byte"></columnOverride>
        </table>

        <!--<table tableName="oa_task_user_file" domainObjectName="oa.OaTaskUserFile" mapperName="oa.OaTaskUserFileMapper">
       </table>-->
        <!--<table tableName="oa_task_remind" domainObjectName="oa.OaTaskRemind" mapperName="oa.OaTaskRemindMapper">
        </table>-->

    </context>
</generatorConfiguration>