<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<!-- Configuration file for UrlRewriteFilter http://www.tuckey.org/urlrewrite/ -->
<urlrewrite>

	<rule>
		<note>cas</note>
		<condition type="method">GET</condition>
		<from>^/m/cas</from>
		<to>/cas</to>
	</rule>
	<!-- 如果服务器使用了nginx动静分离，此处应该创建软链接 -->
	<rule>
		<note>字体</note>
		<condition type="method">GET</condition>
		<from>^/mobile/fonts</from>
		<to>/fonts</to>
	</rule>
	<rule>
		<note>头像</note>
		<condition type="method">GET</condition>
		<from>^/m/avatar</from>
		<to>/avatar</to>
	</rule>
	<rule>
		<note>党员信息查询</note>
		<condition type="method">GET</condition>
		<from>^/m/member_selects</from>
		<to>/member_selects</to>
	</rule>
	<rule>
		<note>干部查询</note>
		<condition type="method">GET</condition>
		<from>^/m/cadre_selects</from>
		<to>/cadre_selects</to>
	</rule>
	<rule>
		<note>优秀年轻干部查询</note>
		<condition type="method">GET</condition>
		<from>^/m/cadreReserve_selects</from>
		<to>/cadreReserve_selects</to>
	</rule>
	<rule>
		<note>单位查询</note>
		<condition type="method">GET</condition>
		<from>^/m/unit_selects</from>
		<to>/unit_selects</to>
	</rule>
	<rule>
		<note>干部任免审批表下载</note>
		<condition type="method">GET</condition>
		<from>^/m/cadreAdform_download</from>
		<to>/cadreAdform_download</to>
	</rule>
	<rule>
		<note>因私出访短信通知弹出框</note>
		<condition type="method">GET</condition>
		<from>^/m/abroad/shortMsg_view$</from>
		<to>/abroad/shortMsg_view</to>
	</rule>
	<rule>
		<note>因私出访短信通知</note>
		<condition type="method">POST</condition>
		<from>^/m/abroad/shortMsg$</from>
		<to>/abroad/shortMsg</to>
	</rule>
	<rule>
		<note>因私出访提交审批结果</note>
		<condition type="method">POST</condition>
		<from casesensitive="true">^/m/abroad/applySelf_approval$</from>
		<to>/abroad/applySelf_approval</to>
	</rule>

	<rule>
		<note>干部请假短信通知弹出框</note>
		<condition type="method">GET</condition>
		<from>^/m/cla/shortMsg_view$</from>
		<to>/cla/shortMsg_view</to>
	</rule>
	<rule>
		<note>干部请假短信通知</note>
		<condition type="method">POST</condition>
		<from>^/m/cla/shortMsg$</from>
		<to>/cla/shortMsg</to>
	</rule>
	<rule>
		<note>干部请假提交审批结果</note>
		<condition type="method">POST</condition>
		<from casesensitive="true">^/m/cla/claApply_approval$</from>
		<to>/cla/claApply_approval</to>
	</rule>
	<rule>
		<note>选课/退课</note>
		<condition type="method">POST</condition>
		<from casesensitive="true">^/m/cet/cetTrain_apply_item$</from>
		<to>/user/cet/cetTrain_apply_item</to>
	</rule>
	<rule>
		<note>文件综合管理</note>
		<condition type="method">POST</condition>
		<from casesensitive="true">^/m/dispatchWorkFile_search$</from>
		<to>/dispatchWorkFile_search</to>
	</rule>
	<rule>
		<note>缴纳党费确认</note>
		<condition type="method">POST</condition>
		<from casesensitive="true">^/m/pmd/payConfirm</from>
		<to>/user/pmd/payConfirm</to>
	</rule>
	<rule>
		<note>党组织查询</note>
		<condition type="method">GET</condition>
		<from>^/m/party_selects</from>
		<to>/party_selects</to>
	</rule>
	<rule>
		<note>支部查询</note>
		<condition type="method">GET</condition>
		<from>^/m/branch_selects</from>
		<to>/branch_selects</to>
	</rule>
	<rule>
		<note>申请入党</note>
		<from>^/m/memberApply</from>
		<to>/user/memberApply</to>
	</rule>
	<rule>
		<note>撤销申请入党</note>
		<condition type="method">POST</condition>
		<from>^/m/applyBack</from>
		<to>/user/applyBack</to>
	</rule>
	<rule>
		<note>组织关系转入</note>
		<from>^/m/memberIn</from>
		<to>/user/memberIn</to>
	</rule>
	<rule>
		<note>恢复组织关系</note>
		<from>^/m/memberReturn</from>
		<to>/user/memberReturn</to>
	</rule>
	<rule>
		<note>流入党员申请</note>
		<from>^/m/memberInflow</from>
		<to>/user/memberInflow</to>
	</rule>
	<rule>
		<note>系统说明</note>
		<condition type="method">GET</condition>
		<from>^/m/hf_content</from>
		<to>/hf_content</to>
	</rule>
</urlrewrite>