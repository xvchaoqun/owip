<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="persistence.ces.common.ICesResultMapper">
    <select id="getCesResultYear" resultType="java.lang.Integer" parameterType="map">
        select year from ces_result
        <where>
            <if test="cadreIds != null and cadreIds.length>0">
                and cadre_id IN
                <foreach collection="cadreIds" item="cadreId" separator="," open="(" close=")">
                    #{cadreId}
                </foreach>
            </if>
        </where>
        group by year order by year desc
    </select>
</mapper>